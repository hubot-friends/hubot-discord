{
  "name": "@hubot-friends/hubot-discord",
  "version": "0.0.0-development",
  "description": "A new Discord adapter for Hubot",
  "main": "index.mjs",
  "scripts": {
    "test": "node --test --test-timeout=10000",
    "start": "node --env-file=.env node_modules/.bin/hubot -f ./index.mjs -n jbot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubot-friends/hubot-discord.git"
  },
  "keywords": [
    "hubot",
    "discord",
    "chat",
    "bot",
    "super",
    "duper",
    "fun"
  ],
  "author": "Joey Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hubot-friends/hubot-discord/issues"
  },
  "homepage": "https://github.com/hubot-friends/hubot-discord#readme",
  "peerDependencies": {
    "hubot": ">= 13"
  },
  "engines": {
    "node": ">= 18"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "dryRun": false
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "discord.js": "^13.17.1"
  }
}
